{
  "format_version": "1.21.90",
  "minecraft:feature_rules": {
    "description": {
      "identifier": "cc:deerfoot.rule",
      "places_feature": "cc:deerfoot_mushroom.feature"
    },
    "conditions": {
      "placement_pass": "after_surface_pass",
      "minecraft:biome_filter": {
        "any_of": [
          { "test": "has_biome_tag", "operator": "==", "value": "forest" },
          { "test": "has_biome_tag", "operator": "==", "value": "birch_forest" },
          { "test": "has_biome_tag", "operator": "==", "value": "dark_forest" },
          { "test": "has_biome_tag", "operator": "==", "value": "cherry_grove" },
          { "test": "has_biome_tag", "operator": "==", "value": "jungle" },
          { "test": "has_biome_tag", "operator": "==", "value": "sparse_jungle" },
          { "test": "has_biome_tag", "operator": "==", "value": "bamboo_jungle" },
          { "test": "has_biome_tag", "operator": "==", "value": "swamp" },
          { "test": "has_biome_tag", "operator": "==", "value": "mangrove_swamp" },
          { "test": "has_biome_tag", "operator": "==", "value": "savanna" },
          { "test": "has_biome_tag", "operator": "==", "value": "taiga" }
        ]
      }
    },
    "distribution": {
      "iterations": 100,
      "scatter_chance": 50.0,
      "x": { "distribution": "uniform", "extent": [-16, 16] },
      "z": { "distribution": "uniform", "extent": [-16, 16] },
      "y": "query.heightmap(variable.worldx, variable.worldz) + math.random_integer(0,1)"
    }
  }
}