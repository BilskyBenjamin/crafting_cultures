{
  "format_version": "1.21.90",
  "minecraft:feature_rules": {
    "description": {
      "identifier": "cc:chaga.rule",
      "places_feature": "cc:chaga_mushroom.feature"
    },
    "conditions": {
      "placement_pass": "after_surface_pass",
      "minecraft:biome_filter": {
        "any_of": [
          { "test": "has_biome_tag", "operator": "==", "value": "birch_forest" },
          { "test": "has_biome_tag", "operator": "==", "value": "forest" }
        ]
      }
    },
    "distribution": {
      "iterations": 100,
      "scatter_chance": 75.0,
      "x": { "distribution": "uniform", "extent": [-16, 16] },
      "z": { "distribution": "uniform", "extent": [-16, 16] },
      "y": "query.heightmap(variable.worldx, variable.worldz) + math.random_integer(3,7)"
    }
  }
}