{
    "format_version": "1.21.90",
        "minecraft:feature_rules": {
            "description": {
                "identifier": "cc:cedar_tree.rule",
                "places_feature": "cc:cedar_tree.feature"
            },
            "conditions": {
                "placement_pass": "after_surface_pass",
            "minecraft:biome_filter": {
                "any_of": [
                    { "test": "has_biome_tag", "operator": "==", "value": "taiga" },
                    { "test": "has_biome_tag", "operator": "==", "value": "old_growth_spruce_taiga" },
                    { "test": "has_biome_tag", "operator": "==", "value": "snowy_taiga" },
                    { "test": "has_biome_tag", "operator": "==", "value": "old_growth_pine_taiga" }
                ]
            }
        },
        "distribution": {
            "iterations": 1,
            "scatter_chance": 5.0,
            "x": { "distribution": "uniform", "extent": [-16, 16] },
            "z": { "distribution": "uniform", "extent": [-16, 16] },
            "y": "query.heightmap(variable.worldx, variable.worldz)"
        }
    }
}